{
    "name": "@adadapted/js-sdk",
    "description": "The AdAdapted JS SDK.",
    "version": "2.4.1",
    "main": "src/index.js",
    "module": "src/index.js",
    "types": "src/**/*.d.ts",
    "scripts": {
        "prettier-fix": "prettier --write \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "lint": "eslint --version && eslint ./",
        "compile": "tsc --version && tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
        "test-coverage": "rm -rf coverage && jest --version && jest --coverage",
        "enable-git-hooks": "git config core.hooksPath .git-hooks",
        "test": "jest --version && jest",
        "prettier": "prettier --list-different \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "npm-check-outdated": "npm outdated",
        "check-peer-dependencies": "npx check-peer-dependencies --npm"
    },
    "keywords": [
        "javascript",
        "adadapted",
        "web",
        "sdk",
        "ad",
        "ads"
    ],
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/adadapted/adadapted-js-sdk"
    },
    "author": "Jeramy Usher <jusher@adadapted.com> (https://gitlab.com/adadapted/adadapted-js-sdk)",
    "license": "AdAdapted Platform License",
    "bugs": {
        "url": "https://gitlab.com/adadapted/adadapted-js-sdk/issues"
    },
    "homepage": "https://gitlab.com/adadapted/adadapted-js-sdk#readme",
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/plugin-transform-typescript": "^7.22.15",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-typescript": "^7.23.2",
        "@peculiar/webcrypto": "^1.4.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/user-event": "^14.5.1",
        "@types/jest": "^29.5.6",
        "@types/jsdom": "^21.1.4",
        "@types/webpack": "^5.28.4",
        "@types/webpack-bundle-analyzer": "^4.6.2",
        "@types/webpack-env": "^1.18.3",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.52.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jest": "^27.4.3",
        "eslint-plugin-jsdoc": "^46.8.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fork-ts-checker-webpack-plugin": "^9.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^22.1.0",
        "mini-css-extract-plugin": "^2.7.6",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.0.3",
        "sass": "^1.69.4",
        "sass-loader": "^13.3.2",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "type-zoo": "^3.3.0",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "webpack-bundle-analyzer": "^4.9.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    }
}
