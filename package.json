{
    "name": "@adadapted/js-sdk",
    "description": "The AdAdapted JS SDK.",
    "version": "2.2.2",
    "main": "src/index.js",
    "module": "src/index.js",
    "types": "src/**/*.d.ts",
    "scripts": {
        "prettier-fix": "prettier --write \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "lint": "eslint --version && eslint ./",
        "compile": "tsc --version && tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
        "test-coverage": "rm -rf coverage && jest --version && jest --coverage",
        "enable-git-hooks": "git config core.hooksPath .git-hooks",
        "test": "jest --version && jest",
        "prettier": "prettier --list-different \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "npm-check-outdated": "npm outdated",
        "check-peer-dependencies": "npx check-peer-dependencies --npm"
    },
    "keywords": [
        "javascript",
        "adadapted",
        "web",
        "sdk",
        "ad",
        "ads"
    ],
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/adadapted/adadapted-js-sdk"
    },
    "author": "Jeramy Usher <jusher@adadapted.com> (https://gitlab.com/adadapted/adadapted-js-sdk)",
    "license": "AdAdapted Platform License",
    "bugs": {
        "url": "https://gitlab.com/adadapted/adadapted-js-sdk/issues"
    },
    "homepage": "https://gitlab.com/adadapted/adadapted-js-sdk#readme",
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/plugin-transform-typescript": "^7.21.3",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.1",
        "@types/jsdom": "^21.1.1",
        "@types/webpack": "^5.28.1",
        "@types/webpack-bundle-analyzer": "^4.6.0",
        "@types/webpack-env": "^1.18.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.4",
        "eslint": "^8.41.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsdoc": "^44.2.5",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fork-ts-checker-webpack-plugin": "^8.0.0",
        "jest": "^29.5.0",
        "jsdom": "^22.0.0",
        "mini-css-extract-plugin": "^2.7.6",
        "postcss-loader": "^7.3.0",
        "prettier": "^2.8.8",
        "sass": "^1.62.1",
        "sass-loader": "^13.3.0",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "type-zoo": "^3.3.0",
        "typescript": "^5.0.4",
        "webpack": "^5.83.1",
        "webpack-bundle-analyzer": "^4.8.0",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.0"
    },
    "jest": {
        "testEnvironmentOptions": {
            "url": "http://localhost"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.ts"
        ],
        "transform": {
            "^.+\\.[j|t]sx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "modulePaths": [
            "<rootDir>/node_modules"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(s?css)$": "<rootDir>/__mocks__/styleMock.js",
            "^src/(.*)$": "<rootDir>/src/$1"
        },
        "collectCoverage": false,
        "coverageDirectory": "coverage",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/api/**/*.*",
            "!src/**/*.d.ts",
            "!src/index.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 5,
                "functions": 5,
                "lines": 5,
                "statements": 5
            }
        },
        "watchPathIgnorePatterns": [
            "<rootDir>/jest.json"
        ]
    }
}
