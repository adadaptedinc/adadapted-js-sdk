{
    "name": "@adadapted/js-sdk",
    "description": "The AdAdapted JS SDK.",
    "version": "2.5.4",
    "main": "src/index.js",
    "module": "src/index.js",
    "types": "src/**/*.d.ts",
    "scripts": {
        "prettier-fix": "prettier --write \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "lint": "eslint --version && eslint ./",
        "compile": "tsc --version && tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
        "test-coverage": "rm -rf coverage && jest --version && jest --coverage",
        "enable-git-hooks": "git config core.hooksPath .git-hooks",
        "test": "jest --version && jest",
        "prettier": "prettier --list-different \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "npm-check-outdated": "npm outdated",
        "check-peer-dependencies": "npx check-peer-dependencies --npm"
    },
    "keywords": [
        "javascript",
        "adadapted",
        "web",
        "sdk",
        "ad",
        "ads"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/adadaptedinc/adadapted-js-sdk"
    },
    "author": "Jeramy Usher <jusher@adadapted.com> (https://github.com/adadaptedinc/adadapted-js-sdk)",
    "license": "AdAdapted Platform License",
    "bugs": {
        "url": "https://github.com/adadaptedinc/adadapted-js-sdk/issues"
    },
    "homepage": "https://docs.adadapted.com/#/",
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/plugin-transform-typescript": "^7.25.2",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-typescript": "^7.24.7",
        "@peculiar/webcrypto": "^1.5.0",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.13",
        "@types/jsdom": "^21.1.7",
        "@types/webpack": "^5.28.5",
        "@types/webpack-bundle-analyzer": "^4.7.0",
        "@types/webpack-env": "^1.18.5",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "eslint": "^8.57.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-jsdoc": "^46.10.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^25.0.0",
        "mini-css-extract-plugin": "^2.9.1",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.3.3",
        "sass": "^1.78.0",
        "sass-loader": "^16.0.1",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "type-zoo": "^3.4.1",
        "typescript": "^5.6.2",
        "webpack": "^5.94.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    }
}
