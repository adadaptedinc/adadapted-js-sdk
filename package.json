{
    "name": "@adadapted/js-sdk",
    "description": "The AdAdapted JS SDK.",
    "version": "2.5.6",
    "main": "src/index.js",
    "module": "src/index.js",
    "types": "src/**/*.d.ts",
    "scripts": {
        "prettier-fix": "prettier --write \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "lint": "eslint --version && eslint ./",
        "compile": "tsc --version && tsc --project tsconfig.json --noEmit --noErrorTruncation --pretty",
        "test-coverage": "rm -rf coverage && jest --version && jest --coverage",
        "enable-git-hooks": "git config core.hooksPath .git-hooks",
        "test": "jest --version && jest",
        "prettier": "prettier --list-different \"*.{ts,tsx,js,jsx,json,css,scss,md}\" \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
        "npm-check-outdated": "npm outdated",
        "check-peer-dependencies": "npx check-peer-dependencies --npm"
    },
    "keywords": [
        "javascript",
        "adadapted",
        "web",
        "sdk",
        "ad",
        "ads"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/adadaptedinc/adadapted-js-sdk"
    },
    "author": "Jeramy Usher <jusher@adadapted.com> (https://github.com/adadaptedinc/adadapted-js-sdk)",
    "license": "AdAdapted Platform License",
    "bugs": {
        "url": "https://github.com/adadaptedinc/adadapted-js-sdk/issues"
    },
    "homepage": "https://docs.adadapted.com/#/",
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/plugin-transform-typescript": "^7.27.1",
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-typescript": "^7.27.1",
        "@eslint/compat": "^1.2.9",
        "@peculiar/webcrypto": "^1.5.0",
        "@stylistic/eslint-plugin-js": "^4.2.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.14",
        "@types/jsdom": "^21.1.7",
        "@types/webpack": "^5.28.5",
        "@types/webpack-bundle-analyzer": "^4.7.0",
        "@types/webpack-env": "^1.18.8",
        "autoprefixer": "^10.4.21",
        "babel-loader": "^10.0.0",
        "copy-webpack-plugin": "^13.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.26.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-jsdoc": "^50.6.11",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fork-ts-checker-webpack-plugin": "^9.1.0",
        "globals": "^16.1.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^26.1.0",
        "mini-css-extract-plugin": "^2.9.2",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.5.3",
        "sass": "^1.87.0",
        "sass-loader": "^16.0.5",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.3.2",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "type-zoo": "^3.4.1",
        "typescript": "^5.8.3",
        "webpack": "^5.99.8",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.1"
    }
}
